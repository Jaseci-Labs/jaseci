    "js-binary": "^1.2.0",
    "motion": "^10.10.0",
    "nanoid": "^3.3.4",
    "parse-json": "^6.0.2",
    "serialize-javascript": "^6.0.0",
    "superjson": "^1.9.1",
    "vis-data": "^7.1.4",
    "vis-network": "^9.1.2",
